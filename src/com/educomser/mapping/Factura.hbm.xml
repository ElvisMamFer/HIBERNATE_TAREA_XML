<?xml version="1.0" encoding="UTF-8"?>


<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

 <class name="com.educomser.entity.Factura" table="factura">
        <id name="cod_factura" column="cod_factura">
            <generator class="identity"/>
        </id>        
        <property name="nit_ci" column="nit_ci" />
        <property name="fecha_factura" />
        <property name="cantidad_total" />
  
  
    </class>

</hibernate-mapping>
